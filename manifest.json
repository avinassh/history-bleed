{
  "manifest_version": 2,
  "name": "History Bleed",
  "version": "0.1",
  "permissions": ["tabs", "notifications", "history", "contextMenus"],
  "background": {
    "scripts": ["jquery.min.js", "background.js"]
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon128.png"
  }
}